#!/bin/sh
# preinst script 
#
# see: dh_installdeb(1)

set -e

case "$1" in
        install|upgrade)
		test -d /etc/kernel/preinst.d && run-parts --arg="PKGVER" /etc/kernel/preinst.d
		exit 0
        ;;

        abort-upgrade)
        ;;

        *)
                echo "preinst called with unknown argument \`$1'" >&2
                exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
